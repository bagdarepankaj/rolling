<h2>Projects</h2>

<div id="projects">
  <% @projects.each do |project| %>
    <%= render project %>
    <p>
      <%= link_to "Show", organization_project_path(@organization, project) %>
    </p>
  <% end %>
</div>

<%= link_to "New Project", new_organization_project_path(@organization || organization) %>
